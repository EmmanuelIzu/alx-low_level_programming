
int checkprime(int n, int m);

int is_prime_number(int n)
{
	return(checkprime(n, 2));
}


int checkprime(int n, int m)
{
	int check = n % m;
if ((m < n) && (check) > 0){
	return (1);
}

if (check == 0 || n < 0 || n == 1)
{
	return (0);
}

if (check == 0 && n == m)
{
	return (0);
}

return (checkprime(n, (m + 1)));
}
